AA,CAAA,QAAA,CAAA,eAAA,EAAA,EAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,gBAAA,EAAA,EAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,iBAAA,EAAA,EAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,gBAAA,EAAA,EAAA;AACA;AATA,GA1BA;AAqCA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,EAAA,OA7CA,qBA6CA;AAAA;;AACA,QAAA,KAAA,MAAA,CAAA,IAAA,IAAA,OAAA,EAAA,KAAA,MAAA,CAAA,QAAA,CAAA,gBAAA,EAAA,EAAA;AACA,IAAA,GAAA,CAAA,GAAA,CAAA,MAAA,EAAA,UAAA,IAAA,EAAA;AACA,MAAA,KAAA,CAAA,SAAA;;AACA,MAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA;AADA,OAAA;AAGA,KALA;AAMA;AArDA,CAAA","sourcesContent":["<template>\n  <div class=\"main\">\n    <navbar></navbar>\n    <div class=\"container\">\n      <sidebar></sidebar>\n      <div class=\"content\">\n        <div class=\"content-path\">\n          <i\n            v-if=\"$route.meta.length > 2\"\n            class=\"el-icon-back\"\n            style=\"color:#999999;padding-right: 10px;cursor: pointer;\"\n            @click=\"goBack\"\n            >返回</i\n          >{{ path }}\n        </div>\n        <div class=\"content-detail\">\n          <router-view></router-view>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport navbar from \"@/pages/layout/navbar\";\nimport sidebar from \"@/pages/layout/sidebar\";\nimport bus from \"@/utils/bus\";\nimport { mapState } from \"vuex\";\nexport default {\n  watch: {\n    $route(to, from) {\n      let path;\n      if (!this.$route.meta) {\n        path = \"\";\n      } else {\n        if (this.$route.meta.length > 1) {\n          path = this.$route.meta.join(\"-\");\n        } else if (this.$route.meta.length == 1) {\n          path = this.$route.meta[0];\n        }\n      }\n      this.$store.dispatch(\"system/setPath\", path);\n    },\n  },\n  computed: {\n    ...mapState({\n      path: (state) => state.system.path,\n      user: (state) => state.login.user,\n    }),\n  },\n  components: {\n    navbar,\n    sidebar,\n  },\n  methods: {\n    goBack(){\n      this.$router.back();\n    },\n    resetUser() {\n      this.$store.dispatch(\"login/setUser\", {});\n      this.$store.dispatch(\"login/setToken\", \"\");\n      this.$store.dispatch(\"login/setRouter\", \"\");\n      this.$store.dispatch(\"system/setPath\", \"\");\n    },\n  },\n  // getDictionaries() {\n  //   this.$axios({\n  //     type: \"GET\",\n  //     url: \"/api-admin/dictType/all\",\n  //   }).then((res) => {\n  //     this.$store.dispatch(\"classes/setDictLists\", res.page);\n  //   });\n  // },\n  mounted() {\n    if (this.$route.path == \"/home\") this.$store.dispatch(\"system/setPath\", \"\");\n    bus.$on(\"goto\", (path) => {\n      this.resetUser();\n      this.$router.push({\n        path,\n      });\n    });\n  },\n};\n</script>\n\n<style lang=\"less\">\n.main {\n  width: 100%;\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n}\n\n.container {\n  display: flex;\n  height: 100%;\n  flex-direction: row;\n}\n\n.content {\n  height: 100%;\n  width: calc(~\"100% - 250px\");\n  border-top: 1px solid #ddd;\n  position: relative;\n\n  &-path {\n    height: 56px;\n    padding-left:20px;\n    line-height: 50px;\n    color: #000;\n    box-shadow: 10px 10px 5px #ccc;\n  }\n\n  &-detail {\n    box-sizing: border-box;\n    // min-height: 600px;\n    height: 100%;\n    background-color: #eff1f3;\n    padding: 20px;\n    .searchModel{\n      background: #fff;\n      margin-bottom: 20px;\n      padding: 20px 0 0 20px;\n    }\n    .opratorBtns{\n      display: flex;\n      justify-content: space-between;\n      background: #fff;\n      padding:20px 20px 10px 20px;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/pages/layout"}]}